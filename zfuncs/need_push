if [ $($git rev-parse --is-inside-work-tree 2>/dev/null) ]
    then
        number=$($git cherry -v origin/$(git symbolic-ref --short HEAD) 2>/dev/null | wc -l | bc)
    if [[ $number == 0 ]]
    then
        echo " "
    else
        echo " with %{$fg_bold[magenta]%}$number unpushed %Bâ‡ˆ%b%{$reset_color%}"
    fi
fi